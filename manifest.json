{
    "manifest_version": 3,
    "name": "Karyam: Simple Task Manager",
    "version": "1.0.1",
    "description": "Helping you to list, observe and prioritise your tasks and get them done.",
    "action": {
        "default_popup": "tasks.html",
        "default_title": "Your today's tasks"
    },
    "permissions": ["storage"],
    "icons": {
        "16": "karyam_icon16.png",
        "48": "karyam_icon48.png",
        "128": "karyam_logo.png"
    }
}
